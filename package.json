{
  "name": "2b2w-ts-rewrite",
  "version": "1.0.0",
  "description": "2b2t queue skipper",
  "repository": "https://github.com/GenerelSchwerz/2b2w-ts-rewrite.git",
  "author": "GenerelSchwerz <xXSG.KingXx@gmail.com>",
  "license": "MIT",

  "main": "lib/index.js",
  "bin": "lib/index.js",
  "scripts": {
    "build": "npx tsc",
    "fix": "npx prettier . -w",
    "test": "ts-node --esm -T ./src/index.ts",
    "start": "node ./lib/index.js",
    "package": "yarn build; pkg package.json; mv ./dist/2b2w-ts-rewrite ../../rust/server_auth_backend/static/2b2w-ts-rewrite-linux-x64"
  },
  "pkg": {
    "targets": [
      "node16-linux-x64"
    ],
    "outputPath": "dist",
    "options": [
      "experimental-modules"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.0",
    "ts-node": "^10.9.1",
    "ts-pkg-scripts": "^0.2.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@rob9315/mcproxy": "^1.0.0",
    "config": "^3.3.8",
    "discord.js": "^14.7.1",
    "discordx": "^11.5.2",
    "dotenv": "^16.0.3",
    "everpolate": "^0.0.3",
    "readline": "^1.3.0",
    "start": "^5.1.0",
    "ts-deepmerge": "^5.0.0",
    "ts-luxon": "^4.0.1"
  }
}
